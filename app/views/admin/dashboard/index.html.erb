<div>
  <h2>Utilisateurs</h2>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= link_to user.email, edit_user_registration_path(user) %>
      </li>
    <% end %>
  </ul>
</div>

<div>
  <h2>Articles</h2>
  <ul>
    <% @items.each do |item| %>
      <li>
        <%= link_to item.title, item_path(item) %>
      </li>
    <% end %>
  </ul>
</div>