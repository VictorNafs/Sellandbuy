<div>
  <h2>Utilisateurs</h2>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= link_to user.email, admin_user_path(user) %>
      </li>
    <% end %>
  </ul>
</div>

<div>
  <h2>Articles</h2>
  <ul>
    <% @items.each do |item| %>
      <li>
        <%= link_to item.title, admin_item_path(item) %>
      </li>
    <% end %>
  </ul>
</div>