<%= link_to "New item", new_item_path %>

<%= link_to ('Categories'), categories_path %> 

<h1>Categories</h1>

  <% @categories.each do |category| %>
    <li><%= link_to category.name, category_id: category.id %></li>
  <% end %>


<h1>Items</h1>

<div id="items">
  <% @items.each do |item| %>
    <%= render item %>
    <p>
      <%= link_to "Show this item", item %>
    </p>
    <p></p>
  <% end %>
</div>



